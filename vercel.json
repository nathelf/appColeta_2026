{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install && npm install --prefix appcoleta-backend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api?path=:path*"
    },
    {
      "source": "/((?!api/|assets/|icons/|favicon|robots).*)",
      "destination": "/index.html"
    }
  ]
}
